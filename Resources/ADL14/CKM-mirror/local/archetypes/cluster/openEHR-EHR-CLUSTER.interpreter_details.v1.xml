  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Interpreter Details</items>
        <items id="description">Details about an Interpreter involved in, or required to be involved in, a clinical activity.</items>
      </items>
      <items code="at0001">
        <items id="text">Interpreter?</items>
        <items id="description">Was an interpreter present during a clinical activity, or will be required for a future clinical activity?</items>
        <items id="comment">Record as True if the interpreter was present during a completed clinical activity, or will be required in a future clinical activity.</items>
      </items>
      <items code="at0002">
        <items id="text">Language</items>
        <items id="description">Identification of the language being interpreted.</items>
      </items>
      <items code="at0003">
        <items id="text">Comment</items>
        <items id="description">Additional narrative description about the interpreter.</items>
        <items id="comment">For example, the interaction with the interpreter was unsatisfactory and so the subject may not understand the content of the discussion with a clinician.</items>
      </items>
      <items code="at0004">
        <items id="text">Interpreter</items>
        <items id="description">Identification of the Interpreter.</items>
        <items id="comment">Only use this SLOT if Participations are not implemented.</items>
      </items>
    </term_definitions>
  </ontology>
