  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Diagnostic Evidence Summary</items>
        <items id="description">Summary of available evidence in support of a diagnosis.</items>
      </items>
      <items code="at0001">
        <items id="text">Clinical Description</items>
        <items id="description">Narrative description of the evidence for the diagnosis.</items>
        <items id="comment">For example: an overview of the evidence, or assertions about previous known infection etc that does not fit into the structured data in this archetype.</items>
      </items>
      <items code="at0002">
        <items id="text">Physical Examination Evidence</items>
        <items id="description">Details about a finding or observation on physical examination that support the diagnosis.</items>
      </items>
      <items code="at0003">
        <items id="text">Examination Finding</items>
        <items id="description">Identification of the finding that supports the diagnosis.</items>
      </items>
      <items code="at0004">
        <items id="text">Finding Description</items>
        <items id="description">Narrative description of the physical finding.</items>
      </items>
      <items code="at0005">
        <items id="text">Result</items>
        <items id="description">Measurement or value associated with the finding.</items>
        <items id="comment">For example: Mantoux test result.</items>
      </items>
      <items code="at0006">
        <items id="text">Date of Examination</items>
        <items id="description">The date on which the physical examination was performed.</items>
      </items>
      <items code="at0007">
        <items id="text">Physical Examination Citation</items>
        <items id="description">Detailed data available about the physical examination held in another part of the health record.</items>
      </items>
      <items code="at0008">
        <items id="text">Laboratory Evidence</items>
        <items id="description">Details about a laboratory result that support the diagnosis.</items>
      </items>
      <items code="at0009">
        <items id="text">Test Result</items>
        <items id="description">The laboratory result that supports the diagnosis.</items>
        <items id="comment">The data type can be specified in a template or at run-time. This field is set optionally to multiple occurrences to allow recording of panels of tests performed on the same date.</items>
      </items>
      <items code="at0010">
        <items id="text">Date of Test</items>
        <items id="description">The date on which the laboratory sample was collected for testing.</items>
      </items>
      <items code="at0011">
        <items id="text">Laboratory Citation</items>
        <items id="description">Detailed data available about the laboratory result held in another part of the health record.</items>
      </items>
      <items code="at0012">
        <items id="text">Imaging Evidence</items>
        <items id="description">Details about a imaging result that support the diagnosis.</items>
      </items>
      <items code="at0013">
        <items id="text">Radiological Diagnosis</items>
        <items id="description">Diagnosis made by clinician on the basis of the imaging test result alone.</items>
      </items>
      <items code="at0014">
        <items id="text">Laboratory Diagnosis</items>
        <items id="description">Diagnosis made by clinician on the basis of the laboratory test result alone.</items>
      </items>
      <items code="at0015">
        <items id="text">Test Result</items>
        <items id="description">The imaging result that supports the diagnosis.</items>
        <items id="comment">The data type can be specified in a template or at run-time. This field is set optionally to multiple occurrences to allow recording of a grouping of related test results performed on the same date.</items>
      </items>
      <items code="at0016">
        <items id="text">Date of Test</items>
        <items id="description">The date on which the image was taken.</items>
      </items>
      <items code="at0017">
        <items id="text">Imaging Citation</items>
        <items id="description">Detailed data available about the imaging result held in another part of the health record.</items>
      </items>
      <items code="at0018">
        <items id="text">Image</items>
        <items id="description">Details about an multimedia representation of the physical examination.</items>
      </items>
      <items code="at0019">
        <items id="text">Image</items>
        <items id="description">Details about an multimedia representation of the laboratory data.</items>
      </items>
      <items code="at0020">
        <items id="text">Image</items>
        <items id="description">Details about an multimedia representation of the imaging data.</items>
      </items>
      <items code="at0021">
        <items id="text">Date Last Updated</items>
        <items id="description">The date at which the diagnostic evidence summary was last clinically asserted, affirmed or confirmed.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

