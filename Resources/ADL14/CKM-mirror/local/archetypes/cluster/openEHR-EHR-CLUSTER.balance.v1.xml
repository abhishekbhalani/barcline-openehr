        <items id="description">Findings observed during the physical examination of balance.</items>
      </items>
      <items code="at0004">
        <items id="text">Eyes Open/closed</items>
        <items id="description">Record of eyes being open or closed during performance of test.</items>
      </items>
      <items code="at0005">
        <items id="text">Eyes open</items>
        <items id="description">Both eyes are open.</items>
      </items>
      <items code="at0006">
        <items id="text">Eyes closed</items>
        <items id="description">Both eyes are closed.</items>
      </items>
      <items code="at0011">
        <items id="text">Affected Side</items>
        <items id="description">Identification of side of abnormal finding.</items>
      </items>
      <items code="at0012">
        <items id="text">Fall towards right</items>
        <items id="description">Falling to the person's right side.</items>
      </items>
      <items code="at0013">
        <items id="text">Fall towards left</items>
        <items id="description">Falling to the person's left side.</items>
      </items>
      <items code="at0014">
        <items id="text">Both sides</items>
        <items id="description">Generalised effect causing loss of balance to both, or either, side.</items>
      </items>
      <items code="at0017">
        <items id="text">Interpretation</items>
        <items id="description">Single word, phrase or brief description represents the clinical meaning and significance of the balance findings.</items>
      </items>
