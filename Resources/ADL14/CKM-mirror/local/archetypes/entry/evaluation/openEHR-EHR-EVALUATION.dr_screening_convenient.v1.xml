    </term_definitions>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">DR screening convenient</items>
        <items id="description">Statement/s about patient's compliance of the requirements established to access to a service of screening for diabetic retinopathy</items>
      </items>
      <items code="at0001">
        <items id="text">Arbol</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Exclusion statement</items>
        <items id="description">Description of the reason of excluding the patient from the screening service.</items>
      </items>
      <items code="at0005">
        <items id="text">Arbol</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0006">
        <items id="text">Date last decision</items>
        <items id="description">The date at which the decision of admittance or exclusion in screening was confirmed.</items>
      </items>
      <items code="at0007">
        <items id="text">Admittance in screening</items>
        <items id="description">Identifies if the patient has been accepted or not to take part in the screening service.</items>
      </items>
      <items code="at0008">
        <items id="text">Screening convenient</items>
        <items id="description">The patient meets the criteria be included in the screening</items>
      </items>
      <items code="at0009">
        <items id="text">Screening not necessary</items>
        <items id="description">The clinician does not consider necessary including the patient into the screening</items>
      </items>
      <items code="at0010">
        <items id="text">Excluded from screening</items>
        <items id="description">Patient definitely excluded from the screening (does not meet the criteria)</items>
