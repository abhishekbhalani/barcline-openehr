      <items code="at0015">
        <items id="text">Excluído del cribado por no disponer de datos de contacto actualizados</items>
        <items id="description">**(es)</items>
      </items>
      <items code="at0016">
        <items id="text">Excluído del cribado por remisión de la diabetes</items>
        <items id="description">**(es)</items>
      </items>
      <items code="at0017">
