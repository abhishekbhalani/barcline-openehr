  <ontology>
    <term_definitions language="pt-br">
      <items code="at0000">
        <items id="text">Meio de Comunicação eletrônica</items>
        <items id="description">Meio de comunicação eletrônica.</items>
      </items>
      <items code="at0001">
        <items id="text">Componentes</items>
        <items id="description">Componentes de um meio de comunicação eletrônica.</items>
      </items>
      <items code="at0003">
        <items id="text">Forma de utilização</items>
        <items id="description">Um código que representa a forma de uso que uma pessoa/organização faz deste meio de comunicação.</items>
      </items>
      <items code="at0004">
        <items id="text">Horário de Utilização</items>
        <items id="description">Uma indicação das preferências de horário de uso para este tipo de contato.</items>
      </items>
      <items code="at0007">
        <items id="text">Detalhes</items>
        <items id="description">Uma combinação única de caracteres usada em um equipamento de comunicação eletrônica com o propósito de estabelecer contato.</items>
      </items>
      <items code="at0009">
        <items id="text">Endereço na internet</items>
        <items id="description">Endereço na internet.</items>
      </items>
      <items code="at0012">
        <items id="text">Identificação do meio de comunicação</items>
        <items id="description">Identificação do telefone, pager, correio eletrônico, etc.</items>
      </items>
      <items code="at0020">
        <items id="text">Telefone</items>
        <items id="description">Indica que se trata de um telefone.</items>
      </items>
      <items code="at0022">
        <items id="text">Celular</items>
        <items id="description">Indica que se trata de um telefone celular.</items>
      </items>
      <items code="at0021">
        <items id="text">Fax</items>
        <items id="description">Indica que se trata de um fax.</items>
      </items>
      <items code="at0023">
        <items id="text">Pager</items>
        <items id="description">Indica que se trata de um pager.</items>
      </items>
      <items code="at0024">
        <items id="text">Correio eletrônico</items>
        <items id="description">Indica que se trata de um correio eletrônico.</items>
      </items>
      <items code="at0025">
        <items id="text">URL</items>
        <items id="description">Indica que se trata de um endereço na internet.</items>
      </items>
      <items code="at0030">
        <items id="text">Uso Pessoal</items>
        <items id="description">Equipamento de uso pessoal.</items>
      </items>
      <items code="at0031">
        <items id="text">Uso no Negócio</items>
        <items id="description">Equipamento de uso profissional/negócio.</items>
      </items>
      <items code="at0032">
        <items id="text">Uso Pessoal/Negócio</items>
        <items id="description">Equipamento de uso pessoal e profissional/negócio.</items>
      </items>
      <items code="at0033">
        <items id="text">Outro uso</items>
        <items id="description">Equipamento para outro tipo de uso. Por exemplo, um telefone para contato, parente, etc.</items>
      </items>
      <items code="at0040">
        <items id="text">Horário comercial</items>
        <items id="description">Indica que este meio de comunicação deve ser utilizado durante o horário comercial.</items>
      </items>
      <items code="at0041">
        <items id="text">Durante o dia</items>
        <items id="description">Indica que este meio de comunicação deve ser utilizado durante o dia.</items>
      </items>
      <items code="at0042">
        <items id="text">Final de semana</items>
        <items id="description">Indica que este meio de comunicação deve ser utilizado aos finais de semana.</items>
      </items>
      <items code="at0043">
        <items id="text">Qualquer hora</items>
        <items id="description">Indica que este meio de comunicação pode ser utilizado a qualquer hora.</items>
      </items>
      <items code="at0044">
        <items id="text">Durante a noite</items>
        <items id="description">Indica que este meio de comunicação deve ser utilizado durante a noite.</items>
      </items>
    </term_definitions>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Electronic address</items>
        <items id="description">The address of an electronic communication medium.</items>
      </items>
      <items code="at0001">
        <items id="text">Components</items>
        <items id="description">Electronic communication medium components.</items>
      </items>
      <items code="at0003">
        <items id="text">Usage</items>
        <items id="description">A code representing the manner of use that a person/organization applies to an electronic medium.</items>
      </items>
      <items code="at0004">
        <items id="text">Time preferences</items>
        <items id="description">An indication of the time of day preference for use of this electronic contact address.</items>
      </items>
      <items code="at0007">
        <items id="text">Details</items>
        <items id="description">A unique combination of characters used as input to electronic telecommunication equipment for the purpose of establishing contact.</items>
      </items>
      <items code="at0009">
        <items id="text">Internet address</items>
        <items id="description">Internet address.</items>
      </items>
      <items code="at0012">
        <items id="text">Identification of the electronic communication medium</items>
        <items id="description">Identification of telephone, fax, pager, e-mail, etc.</items>
      </items>
      <items code="at0020">
        <items id="text">Telephone</items>
        <items id="description">The medium is a telephone (not a mobile phone).</items>
      </items>
      <items code="at0021">
        <items id="text">Mobile</items>
        <items id="description">The medium is a mobile phone (not a landline).</items>
      </items>
      <items code="at0022">
        <items id="text">Fax</items>
        <items id="description">The medium is a faxsimile machine.</items>
      </items>
      <items code="at0023">
        <items id="text">Pager</items>
        <items id="description">The medium is a pager.</items>
      </items>
      <items code="at0024">
        <items id="text">E-mail</items>
        <items id="description">The medium is e-mail.</items>
      </items>
      <items code="at0025">
        <items id="text">URL</items>
        <items id="description">The medium is an internet address.</items>
      </items>
      <items code="at0030">
        <items id="text">Personal</items>
        <items id="description">The medium is for personal use.</items>
      </items>
      <items code="at0031">
        <items id="text">Business</items>
        <items id="description">The medium is for business use.</items>
      </items>
      <items code="at0032">
        <items id="text">Personal/Business</items>
        <items id="description">The medium is for both business and personal use.</items>
      </items>
      <items code="at0033">
        <items id="text">Other use</items>
        <items id="description">The medium is for another use, e.g., a telephone for contact, a relative's phone.</items>
      </items>
      <items code="at0040">
        <items id="text">Use during business hours</items>
        <items id="description">The medium is accessible during the business hours.</items>
      </items>
      <items code="at0041">
        <items id="text">Use during day time</items>
        <items id="description">The medium should be used during day time hours.</items>
      </items>
      <items code="at0042">
        <items id="text">Use at weekend</items>
        <items id="description">The medium should be used at the weekend.</items>
      </items>
      <items code="at0043">
        <items id="text">Use at any time</items>
        <items id="description">The medium can be used at all times.</items>
      </items>
      <items code="at0044">
        <items id="text">Use during evening/night.</items>
        <items id="description">The medium should be used during the evening or at night.</items>
      </items>
    </term_definitions>
