  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Psychiatric Risk Assessment (NT RMP)</items>
        <items id="description">Risk Assessment Tool to determine the potential risk of transporting patients with psychiatric issues to a treatment facility by aeroplane. This assessment tool is specifically used with the Remote Medical Practitioner On-Call service in the Northern Territory, Australia.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Marked Disorientation</items>
        <items id="description">The subject appears to be markedly disoriented.</items>
        <items id="comment">Record as True, if the subject is markedly disoriented.</items>
      </items>
      <items code="at0003">
        <items id="text">Marked Agitation</items>
        <items id="description">The subject appears to be markedly agitated.</items>
        <items id="comment">Record as True, if the subject is markedly disoriented.</items>
      </items>
      <items code="at0004">
        <items id="text">Current Violence to Persons or Property</items>
        <items id="description">The subject has been violent towards persons or property.</items>
        <items id="comment">Record as True, if the subject has been violent towards persons or property.</items>
      </items>
      <items code="at0005">
        <items id="text">Current Violent Ideation or Threats of Violence</items>
        <items id="description">The subject has been threatening violence towards persons or property.</items>
        <items id="comment">Record as True, if the subject has been threatening violence towards persons or property.</items>
      </items>
      <items code="at0006">
        <items id="text">Current Self Harm or Intention</items>
        <items id="description">The subject has deliberately self harmed or is threatening self harm.</items>
        <items id="comment">Record as True, if the subject has deliberately self harmed or is threatening self harm.</items>
      </items>
      <items code="at0007">
        <items id="text">History of Violence</items>
        <items id="description">The subject has a known history of known violence.</items>
        <items id="comment">Record as True, if the subject has a known history of violence.</items>
      </items>
      <items code="at0008">
        <items id="text">Previous History of Difficulties During Transportation</items>
        <items id="description">The subject has a known history of difficulties during transportation.</items>
        <items id="comment">Record as True, if the subject has a known previous history of having difficulties during transportation.</items>
      </items>
      <items code="at0009">
        <items id="text">Involuntary Patient</items>
        <items id="description">The subject has been placed  under an involuntary treatment order.</items>
        <items id="comment">Record as True, if the subject has been placed  under an involuntary treatment order.</items>
      </items>
      <items code="at0010">
        <items id="text">Known to Threaten to Use Weapons</items>
        <items id="description">The subject has been known to threaten to use weapons.</items>
        <items id="comment">Record as True, if the subject has been known to threaten to use weapons.</items>
      </items>
      <items code="at0011">
        <items id="text">Referral Facility Unable to Achieve Recommended Management</items>
        <items id="description">The referral facility has been unable to achieve recommended treatment.</items>
        <items id="comment">Record as True, if the referral facility has been unable to achieve recommended treatment.</items>
      </items>
      <items code="at0012">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0013">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
    </term_definitions>
  </ontology>
