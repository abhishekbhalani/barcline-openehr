  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Rinne and Weber Test Results</items>
        <items id="description">Two hearing screening tests, commonly recorded together to determine the presence and character of any detected hearing loss.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Weber Test Result</items>
        <items id="description">A test to compare bone conduction in both ears.</items>
      </items>
      <items code="at0005">
        <items id="text">No Lateralisation</items>
        <items id="description">The sound from vibration is heard equally in both ears.</items>
      </items>
      <items code="at0006">
        <items id="text">Lateralising Left</items>
        <items id="description">The sound from the vibration is heard predominantly on the left.</items>
      </items>
      <items code="at0007">
        <items id="text">Lateralising Right</items>
        <items id="description">The sound from the vibration is heard predominantly on the right.</items>
      </items>
      <items code="at0008">
        <items id="text">Rinne Test</items>
        <items id="description">A test to compare the subject's perception of sounds transmitted by air conduction to those transmitted by bone conduction through the mastoid.</items>
      </items>
      <items code="at0009">
        <items id="text">Rinne Test Result</items>
        <items id="description">The result of the Rinne Test.</items>
      </items>
      <items code="at0010">
        <items id="text">Ear Tested</items>
        <items id="description">Identification of the ear being tested.</items>
      </items>
      <items code="at0011">
        <items id="text">Left</items>
        <items id="description">The left ear was tested.</items>
      </items>
      <items code="at0012">
        <items id="text">Right</items>
        <items id="description">The right ear was tested.</items>
      </items>
      <items code="at0013">
        <items id="text">Negative</items>
        <items id="description">Air Conduction is less than bone conduction.</items>
      </items>
      <items code="at0014">
        <items id="text">Positive</items>
        <items id="description">Air Conduction is greater than bone conduction.</items>
      </items>
      <items code="at0017">
        <items id="text">Clinical Interpretation</items>
        <items id="description">Single word, phrase or brief description represents the clinical meaning and significance of the physical examination findings.</items>
      </items>
      <items code="at0018">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the Rinne and Weber tests not captured in other fields.</items>
      </items>
    </term_definitions>
  </ontology>
