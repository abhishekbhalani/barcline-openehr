  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Palpation of Uterus</items>
        <items id="description">Examination of the uterus by palpation, using either an abdominal or vaginal approach.</items>
      </items>
      <items code="at0001">
        <items id="text">Clinical Description</items>
        <items id="description">Narrative description of findings on examination of the uterus.</items>
      </items>
      <items code="at0002">
        <items id="text">Position</items>
        <items id="description">Position of uterus.</items>
      </items>
      <items code="at0003">
        <items id="text">Anteverted</items>
        <items id="description">Uterine fundus is tipped forward.</items>
      </items>
      <items code="at0004">
        <items id="text">Retroverted</items>
        <items id="description">Uterine fundus is tipped backward.</items>
      </items>
      <items code="at0005">
        <items id="text">Fundal Height</items>
        <items id="description">The height of the uterine fundus measured from the symphysis pubis.</items>
        <items id="comment">Usually recorded in pregnancy after approximately 20 weeks of gestation.</items>
      </items>
      <items code="at0006">
        <items id="text">Gestation</items>
        <items id="description">Uterine size estimation, expressed as length of gestation.</items>
      </items>
      <items code="at0007">
        <items id="text">Uterine size</items>
        <items id="description">Measurement or estimation of the size of the uterus.</items>
      </items>
      <items code="at0008">
        <items id="text">Relative Size</items>
        <items id="description">Estimated size of the uterus relative to commonly used anatomical markers, estimations of pregnancy gestation (even if non-pregnant) or common objects.</items>
      </items>
      <items code="at0009">
        <items id="text">At pubic symphysis</items>
        <items id="description">The fundus of the uterus is just palpable above the pubic symphysis on abdominal examination, and approximately equivalent to the size of a pregnant uterus at 12 weeks.</items>
      </items>
      <items code="at0010">
        <items id="text">At umbilicus</items>
        <items id="description">The fundus of the uterus is palpable at, or about, the level of the umbilicus.</items>
      </items>
      <items code="at0011">
        <items id="text">At xiphisternum</items>
        <items id="description">The fundus of the uterus is palpable at, or about, the xiphisternum.</items>
      </items>
      <items code="at0012">
        <items id="text">Liquor Volume</items>
        <items id="description">Assessment of the relative amount of liquor present in the uterus.</items>
      </items>
      <items code="at0014">
        <items id="text">Decreased</items>
        <items id="description">The amount of liquor appears reduced; less than expected for gestation.</items>
      </items>
      <items code="at0016">
        <items id="text">Normal</items>
        <items id="description">The amount of liquor appears normal; as expected for gestation.</items>
      </items>
      <items code="at0018">
        <items id="text">Increased</items>
        <items id="description">The amount of liquor appears increased; more than expected for gestation.</items>
      </items>
      <items code="at0020">
        <items id="text">Number of Fetuses</items>
        <items id="description">The number of fetuses found on palpation of the uterus.</items>
      </items>
      <items code="at0022">
        <items id="text">Fetal Detail</items>
        <items id="description">Detailed information about the fetus. Record details about each fetus in a multiple birth separately.</items>
      </items>
      <items code="at0023">
        <items id="text">10 weeks gestation</items>
        <items id="description">Uterus is obviously enlarged - approximately equivalent to the size of a grapefruit or a pregnant uterus of 10 weeks gestation.</items>
      </items>
      <items code="at0024">
        <items id="text">8 weeks gestation</items>
        <items id="description">Uterus is moderately enlarged - approximately equivalent to the size of a pregnant uterus at 8 weeks gestation.</items>
      </items>
      <items code="at0025">
        <items id="text">6 weeks gestation</items>
        <items id="description">Uterus is slightly enlarged - approximately equivalent to the size of a pregnant uterus at 6 weeks gestation.</items>
      </items>
      <items code="at0026">
        <items id="text">Normal size</items>
        <items id="description">Uterus is approximately the size of a plum or a normal, non-pregnant uterus.</items>
      </items>
      <items code="at0027">
        <items id="text">Surface</items>
        <items id="description">Narrative description of the surface and shape of the uterus.</items>
      </items>
      <items code="at0028">
        <items id="text">Consistency</items>
        <items id="description">Narrative description of the consistency of the uterus.</items>
      </items>
      <items code="at0029">
        <items id="text">Tenderness</items>
        <items id="description">Narrative description of any findings of tenderness.</items>
        <items id="comment">True if the uterus is tender. Not to include tenderness of the adnexae or cervix or pelvic region in general.</items>
      </items>
      <items code="at0030">
        <items id="text">Mobility</items>
        <items id="description">Mobility of the uterus.</items>
        <items id="comment">Consistent with SNOMED values.</items>
      </items>
      <items code="at0036">
        <items id="text">Mobile</items>
        <items id="description">Uterus is moveable.</items>
      </items>
      <items code="at0037">
        <items id="text">Fixed</items>
        <items id="description">Uterus is fixed and immobile.</items>
      </items>
      <items code="at0038">
        <items id="text">Upright</items>
        <items id="description">Uterine fundus is neither anteverted nor retroverted.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

