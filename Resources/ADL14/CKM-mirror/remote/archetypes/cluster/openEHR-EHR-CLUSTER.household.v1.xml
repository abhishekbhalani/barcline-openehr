  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Household</items>
        <items id="description">Summary of the people and pets with whom the subject lives.</items>
      </items>
      <items code="at0001">
        <items id="text">Category</items>
        <items id="description">Categorisation of a household based on those residing in the same dwelling and making common provision for living.</items>
        <items id="comment">Coding with a terminology is desirable, where possible. For example:  lives alone or lives with others; family or non-family; single or group.</items>
      </items>
      <items code="at0002">
        <items id="text">Description</items>
        <items id="description">Narrative description about the household members.</items>
        <items id="comment">For example: a full detailed description of individuals and relatioships; or  'living with spouse, mother-in-law and 4 dependant children'; or 'living with friends'.</items>
      </items>
      <items code="at0003">
        <items id="text">All Occupants</items>
        <items id="description">Total number of people living in the household.</items>
      </items>
      <items code="at0004">
        <items id="text">Dependents</items>
        <items id="description">Number of dependents of this subject in the household.</items>
      </items>
      <items code="at0006">
        <items id="text">Children Under 5 years</items>
        <items id="description">Number of children under the age of 5 years in the household.</items>
      </items>
    </term_definitions>
  </ontology>
