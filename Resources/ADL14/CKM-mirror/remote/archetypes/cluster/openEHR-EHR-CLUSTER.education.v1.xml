  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Education and Training</items>
        <items id="description">Summary of the educational and training background of the subject.</items>
      </items>
      <items code="at0002">
        <items id="text">Highest Achieved</items>
        <items id="description">Description of highest level of education or training achieved to date.</items>
        <items id="comment">Coding with a terminology is desirable, where possible. For example: completed secondary school; completed postgraduate degree; completed apprenticeship; or never attended school.</items>
      </items>
      <items code="at0003">
        <items id="text">Age Commenced Formal Education</items>
        <items id="description">Age when the individual commenced formal education.</items>
      </items>
      <items code="at0004">
        <items id="text">Milestone</items>
        <items id="description">Name of the education or training milestone or academic qualification achieved.</items>
      </items>
      <items code="at0005">
        <items id="text">Milestone Achieved</items>
        <items id="description">Identification of educational or training milestones acheived.</items>
      </items>
      <items code="at0006">
        <items id="text">Date Achieved</items>
        <items id="description">Date that education milestone was achieved.</items>
      </items>
      <items code="at0007">
        <items id="text">Comment</items>
        <items id="description">Overall comment about the individual's education.</items>
      </items>
      <items code="at0011">
        <items id="text">Attendance</items>
        <items id="description">Description of attendance at educational facility/location.</items>
        <items id="comment">Only applicable for primary and secondary age children. Reference: METeOR - http://meteor.aihw.gov.au/content/index.phtml/itemId/401809</items>
      </items>
      <items code="at0012">
        <items id="text">Enrolled; Attending</items>
        <items id="description">The subject is currently enrolled and attending school regularly.</items>
      </items>
      <items code="at0013">
        <items id="text">Enrolled; Not Always Attending</items>
        <items id="description">The subject is currently enrolled and is not attending school regularly.</items>
      </items>
      <items code="at0014">
        <items id="text">Enrolled; Not Attending</items>
        <items id="description">The subject is currently enrolled and is not attending school at all.</items>
      </items>
      <items code="at0015">
        <items id="text">Enrolled; Waiting to Commence</items>
        <items id="description">The subject is currently enrolled but is waiting to commence.</items>
      </items>
      <items code="at0016">
        <items id="text">Home Education</items>
        <items id="description">The subject is currently educated at home.</items>
      </items>
      <items code="at0017">
        <items id="text">Not Enrolled; Not Home Education</items>
        <items id="description">The subject is currently not enrolled and is not educated at home.</items>
      </items>
      <items code="at0018">
        <items id="text">Description</items>
        <items id="description">Narrative description of education and training, including current activity and/or past experience.</items>
      </items>
      <items code="at0019">
        <items id="text">Place</items>
        <items id="description">Facility or institution at which milestone was achieved.</items>
      </items>
      <items code="at0020">
        <items id="text">Category</items>
        <items id="description">Category of education that is appropriate for a child subject to attend, based on age and ability.</items>
      </items>
      <items code="at0021">
        <items id="text">Not Applicable</items>
        <items id="description">It is not appropriate for this subject to attend.</items>
      </items>
      <items code="at0022">
        <items id="text">Childcare</items>
        <items id="description">The subject is of the appropriate age and ability to attend a Childcare facility.</items>
