  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Housing</items>
        <items id="description">Summary of the housing or accomodation in which the subject resides.</items>
      </items>
      <items code="at0001">
        <items id="text">Homeless</items>
        <items id="description">Is the subject homeless?</items>
        <items id="comment">Record as 'True' if individual is homeless.</items>
      </items>
      <items code="at0002">
        <items id="text">Category</items>
        <items id="description">Type of accommodation where the subject usually resides.</items>
        <items id="comment">Coding with a terminology is desirable, where possible. For example: a flat or apartment; caravan; or nursing home.</items>
      </items>
      <items code="at0003">
        <items id="text">Tenure</items>
        <items id="description">The type of legal right to occupy a dwelling, for example, owner or renter.</items>
      </items>
      <items code="at0004">
        <items id="text">Description</items>
        <items id="description">Description of the housing or accommodation.</items>
      </items>
      <items code="at0005">
        <items id="text">Support Provided</items>
        <items id="description">Description of additional support provided as part of the housing/accommodation arrangement.</items>
        <items id="comment">For example: nursing staff on-site overnight.</items>
      </items>
      <items code="at0006">
        <items id="text">Detail</items>
        <items id="description">Additional details about housing.</items>
        <items id="comment">For example: disability aids installed; number of stairs; or availability of refrigerator in which to store medications.</items>
      </items>
      <items code="at0007">
        <items id="text">Transient</items>
        <items id="description">Is the subject moving between multiple home environements?</items>
      </items>
    </term_definitions>
  </ontology>
