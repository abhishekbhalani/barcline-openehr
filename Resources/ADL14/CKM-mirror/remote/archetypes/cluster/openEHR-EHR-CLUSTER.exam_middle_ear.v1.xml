    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Examination of Middle Ear</items>
        <items id="description">Examination of the middle ear, usually at operation.</items>
      </items>
      <items code="at0001">
        <items id="text">Ear Examined</items>
        <items id="description">Identification of the ear under examination.</items>
      </items>
      <items code="at0002">
        <items id="text">Left</items>
        <items id="description">The left middle ear.</items>
      </items>
      <items code="at0003">
        <items id="text">Right</items>
        <items id="description">The right middle ear.</items>
      </items>
      <items code="at0004">
        <items id="text">Description</items>
        <items id="description">Narrative description of findings observed during examination of the middle ear.</items>
      </items>
      <items code="at0005">
        <items id="text">Ossicular Chain State</items>
        <items id="description">Description of status of the ossicular chain.</items>
      </items>
      <items code="at0006">
        <items id="text">Intact</items>
        <items id="description">Ossicular chain is intact.</items>
      </items>
      <items code="at0007">
        <items id="text">Not Intact</items>
        <items id="description">Ossicular chain is not intact.</items>
      </items>
      <items code="at0008">
        <items id="text">Ossicular Chain Mobility</items>
        <items id="description">Description of the mobility of the ossicular chain.</items>
      </items>
      <items code="at0009">
        <items id="text">Mobile</items>
        <items id="description">Ossicular chain is mobile.</items>
      </items>
      <items code="at0010">
        <items id="text">Immobile</items>
        <items id="description">Ossicular chain is not mobile.</items>
      </items>
      <items code="at0011">
        <items id="text">Clinical Interpretation</items>
        <items id="description">Single word, phrase or brief description representing a summary of the examination findings.</items>
      </items>
      <items code="at0012">
        <items id="text">No Abnormality Detected</items>
        <items id="description">Statement that no abnormality was detected on examination of the middle ear.</items>
        <items id="comment">Record as True if no abnormality was detected on examination.  Specific statements about the examination can be included in the 'Clinical Interpretation' data element.</items>
      </items>
    </term_definitions>
