      <items code="at0001">
        <items id="text">Working Refrigerator</items>
        <items id="description">Presence of a working refrigerator in the home.</items>
        <items id="comment">For example: required to store medicines adequately, especially in a remote environment or tropical climate.</items>
      </items>
      <items code="at0002">
        <items id="text">Description</items>
